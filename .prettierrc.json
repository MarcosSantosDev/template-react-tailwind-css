{
	"plugins": [
		"prettier-plugin-tailwindcss"
	],
	"endOfLine": "lf",
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"printWidth": 120,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"singleAttributePerLine": true
}